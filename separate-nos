 //wap to separate positive negative and zero
 #include<stdio.h>
 int main()
 {
 int a[100];
 int n,i,l=0;
 printf("enter the limit \n");
 scanf("%d",&n);
 printf("enter elements \n");
 for(i=0;i<n;i++)
 {scanf("%d",&a[i]);
 }
 	 for(i=0;i<n;i++)
 	 {
 	 if(a[i]<0)
 	 {
 	 for(int j =i; j>=l;j--)
 	 {
 	 int t =a[j];
 	 a[j]= a[j-1];
 	 a[j-1]=t;
 	 }
 	 l++;
 	 }
 	 }
 	 
 	 l++;
 	 
 	 for(i=0;i<n;i++)
 	 {
 	 if(a[i]==0)
 	 {
 	 for(int j =i; j>=l;j--)
 	 {
 	 int t =a[j];
 	 a[j]= a[j-1];
 	 a[j-1]=t;
 	 }
 	 l++;
 	 }
 	 }
 for(i=0;i<n;i++)
 { 
 printf("%d	",a[i]);
}
 return 0;
 }
 	 
 
 
